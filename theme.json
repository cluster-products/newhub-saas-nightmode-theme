{
  "object": {
    "title": {
      "main": {
        "color": "@TitleColor",
        "fontSize": "@TitleSize"
      },
      "subTitle": {
        "color": "@SubtitleColor",
        "fontSize": "@SubtitleSize"
      },
      "footer": {
        "color": "@FooterColor",
        "fontSize": "@FooterSize",
        "backgroundColor": "#ffffff"
      }
    },
    "label": {
      "name": {
        "color": "@TextColor",
        "fontSize": "@TextSize"
      },
      "value": {
        "color": "@TextColor",
        "fontSize": "@TextSize"
      }
    },
    "axis": {
      "title": {
        "color": "@TextColor",
        "fontSize": "@TextSize"
      },
      "label": {
        "name": {
          "color": "@TextColor",
          "fontSize": "@TextSize"
        }
      },
      "line": {
        "major": {
          "color": "#2c2c2c"
        },
        "minor": {
          "color": "#2c2c2c"
        }
      }
    },
    "grid": {
      "line": {
        "highContrast": {
          "color": "#2c2c2c"
        },
        "major": {
          "color": "#2c2c2c"
        },
        "minor": {
          "color": "#2c2c2c"
        }
      }
    },
    "referenceLine": {
      "label": {
        "name": {
          "color": "@TextColor",
          "fontSize": "@TextSize"
        }
      },
      "outOfBounds": {
        "color": "@TextColor",
        "backgroundColor": "#ffffff",
        "fontSize": "@TextSize"
      }
    },
    "legend": {
      "title": {
        "color": "@TextColor",
        "fontSize": "@TextSize"
      },
      "label": {
        "color": "@TextColor",
        "fontSize": "@TextSize"
      }
    }
  },
  "customStyles": [
    {
      "cssRef": "theme.css",
      "classRef": "nightmode"
    }
  ],
  "_inherit": false,
  "_variables": {
    "@TitleSize": "16px",
    "@SubtitleSize": "15px",
    "@TextSize": "12px",
    "@FooterSize": "10px",
    "@TitleColor": "#cccccc",
    "@SubtitleColor": "#cfcfcf",
    "@TextColor": "#cccccc",
    "@FooterColor": "#ffffff",
    "@BackgroundColor": "#f5faff",
    "@AxisMajorColor": "#cccccc",
    "@AxisMinorColor": "#cccccc"
  },
  "color": "@TextColor",
  "fontSize": "@TextSize"
}